public static Integer arraySum(List<Integer> numbers) {
    //code here

    if(numbers == null)
    {
        return null;
    }
    else
    {
        Integer sum = 0;
        Integer arr = numbers.size();
        for(Integer i = 0;i<arr;i++)
        {
            sum += numbers.[i];
        }
        return sum;
    }
}